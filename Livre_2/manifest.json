{
  "name": "Ecidade",
  "version": "1.0",
  "manifest_version": 2,
  "description": "logs",
  "browser_action": {
      "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["background.js"],
      "persistent":true
  },
 "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["<all_urls>",
      "webNavigation",
      "tabs","storage"
  ]
}